# Project .gitignore configuration

# Built application files
*.apk
*.ap_

# Files for the dex VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/
build/
generated/

# Local configuration file (sdk path, etc)
local.properties

# Eclipse project files
.classpath
.project

# Mac files
.DS_Store

# IDEA/Android Studio project files, because the project can be imported from settings.gradle.kts
*.iml
#misc.xml
#deploymentTargetDropDown.xml
#render.experimental.xml
.idea/*
#!.idea/copyright
# Keep the code styles.
!/.idea/codeStyles
/.idea/codeStyles/*
!/.idea/codeStyles/Project.xml
!/.idea/codeStyles/codeStyleConfig.xml

# Gradle cache
.gradle

# Sandbox stuff
_sandbox

# Android Studio generated files and folders
.externalNativeBuild/
.cxx/
output.json
# Android Studio captures folder
captures/

# Kotlin
.kotlin

# Log/OS Files
*.log

# Keystore files
*.jks
*.keystore

# Google Services (e.g. APIs or Firebase)
google-services.json

# Android Profiling
*.hprof